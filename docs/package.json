{
    "name": "@wallet-ui/docs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev --turbo --port 3080",
        "start": "next start",
        "postinstall": "fumadocs-mdx",
        "compile:js": "opennextjs-cloudflare build",
        "preview": "npm run compile:js && opennextjs-cloudflare preview",
        "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
        "style:fix": "next lint --fix && pnpm prettier --log-level warn --ignore-unknown --write ./*",
        "test:lint": "next lint",
        "test:prettier": "TERM_OVERRIDE=\"${TURBO_HASH:+dumb}\" TERM=${TERM_OVERRIDE:-$TERM} jest -c ../node_modules/@wallet-ui/test-config/jest-prettier.config.ts --rootDir . --silent",
        "test:typecheck": "tsc"
    },
    "dependencies": {
        "@opennextjs/cloudflare": "^1.2.1",
        "@wallet-ui/react": "workspace:*",
        "@wallet-ui/tailwind": "workspace:*",
        "fumadocs-core": "15.5.1",
        "fumadocs-docgen": "^2.0.0",
        "fumadocs-mdx": "11.6.7",
        "fumadocs-ui": "15.5.1",
        "lucide-react": "^0.514.0",
        "next": "15.3.3",
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.10",
        "@types/mdx": "^2.0.13",
        "@types/node": "24.0.1",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "eslint": "^9.28.0",
        "eslint-config-next": "15.3.3",
        "postcss": "^8.5.5",
        "tailwindcss": "^4.1.10",
        "typescript": "^5.8.3",
        "wrangler": "^4.20.0"
    }
}
